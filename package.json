{
  "name": "@eropple/nestjs-openapi3",
  "version": "0.4.4",
  "description": "OpenAPI 3.x document generation, presentation, and validation for NestJS.",
  "main": "dist",
  "tags": [
    "nest",
    "nestjs",
    "openapi",
    "oas3",
    "swagger"
  ],
  "author": "Ed Ropple",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eropple/nestjs-openapi3"
  },
  "private": false,
  "scripts": {
    "build": "yarn exec -- tsc",
    "watch": "yarn build --watch",
    "prepare": "yarn build",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "peerDependencies": {
    "@nestjs/common": "6.5.2",
    "@nestjs/core": "6.5.2",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3"
  },
  "devDependencies": {
    "@nestjs/common": "6.5.2",
    "@nestjs/core": "6.5.2",
    "@types/cookie": "0.3.3",
    "@types/lodash": "4.14.135",
    "@types/node": "10.12.18",
    "@types/voca": "1.4.0",
    "jest": "24.9.0",
    "prettier": "1.15.3",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "swagger-ui-express": "4.1.1",
    "ts-jest": "24.0.2",
    "tslint": "5.16.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@eropple/bunyan-wrapper": "0.1.2",
    "@eropple/find-up": "0.1.0",
    "@types/express": "4.17.1",
    "bunyan": "1.8.12",
    "cookie": "0.4.0",
    "lodash": "4.17.19",
    "openapi-enforcer": "1.5.1",
    "openapi3-ts": "1.3.0",
    "voca": "1.4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
